 DELETE {{ DFH_ZOS_STCJOBS }}({{ DFH_REGION_APPLID }}) FILE(DD1)
 /* Stcjobs member may not have been created so tolerate RC=8 */
 IF LASTCC = 8 THEN DO
   SET MAXCC=0
 END
 /* For all data sets, only attempt delete if data set exists */
 /* so that an RC=8 for other reasons can be spotted and      */
 /* treated as a failure during deprovision                   */
