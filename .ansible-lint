---
var_naming_pattern: "^[A-Za-z_][A-Za-z0-9_]*$"
skip_list:
  - var-naming[no-role-prefix]
  - var-naming[no-jinja] # zos_job_submit_template role uses a dynamic variable name
exclude_paths:
  - collections
  - .venv
